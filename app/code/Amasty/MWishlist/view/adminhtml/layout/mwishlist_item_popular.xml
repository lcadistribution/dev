                    <argument name="viewModel" xsi:type="object">Amasty\MWishlist\ViewModel\Analytics\Purchased</argument>
                </arguments>
            </block>
            <uiComponent name="amasty_mwishlist_popular_listing" />
        </referenceContainer>
    </body>
</page>
