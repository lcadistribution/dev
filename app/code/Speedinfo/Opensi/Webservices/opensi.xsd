<xsd:schema
	attributeFormDefault="qualified"
	elementFormDefault="qualified"
	targetNamespace="http://www.opensi.fr/xsd"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://www.opensi.fr/xsd"
	>
	<xsd:complexType name="Attributes">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[List of the different attributes of a product (Store => OpenSi).]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="unbounded" name="Attribute" nillable="true" type="tns:Attribute" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Attribute">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[List of the available fields for sending the attributes of a product in OpenSi (Store => OpenSi).]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" name="Name" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Value" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Product_fromStore">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
					List of the available fields for sending a new product in OpenSi (Store => OpenSi).
					A product entry must have at least
						- Reference
						- Name
						- Price
						- VATRate
					to be created in OpenSi (required fields).
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" name="Reference" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Brand" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Name" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="ShortDescription" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Description" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="ModelReference" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="IncludingVATPriceDefinition" nillable="true" type="xsd:boolean" />
			<xsd:element minOccurs="0" maxOccurs="1" name="WholesalePrice" nillable="true" type="xsd:double" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Price" nillable="true" type="xsd:double" />
			<xsd:element minOccurs="0" maxOccurs="1" name="RetailPrice" nillable="true" type="xsd:double" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Ecotax" nillable="true" type="xsd:double" />
			<xsd:element minOccurs="1" maxOccurs="1" name="VATRate" type="xsd:double" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Barcode" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Attribute_1" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Attribute_2" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Attribute_3" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Attribute_4" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Attribute_5" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Attribute_6" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Attributes" nillable="true" type="tns:Attributes" />
      <xsd:element minOccurs="0" maxOccurs="1" name="Family_1" nillable="true" type="xsd:string" />
      <xsd:element minOccurs="0" maxOccurs="1" name="Family_2" nillable="true" type="xsd:string" />
      <xsd:element minOccurs="0" maxOccurs="1" name="Family_3" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Volume" type="xsd:double" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Height" type="xsd:double" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Length" type="xsd:double" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Width" type="xsd:double" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Weight" type="xsd:double" />
			<xsd:element minOccurs="0" maxOccurs="1" name="ManufacturerReference" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="NC8Code" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="CountryCodeManufacture" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="NetWeight" type="xsd:double" />
			<xsd:element minOccurs="0" maxOccurs="1" name="PickingZone" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="ProductType" nillable="true" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProductUpdate_fromStore">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
					List of the available fields for updating a product in OpenSi (Store => OpenSi).
					A product entry must have at least
						- Reference
						- Name
					to be updated in OpenSi (required fields).
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" name="Reference" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Brand" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Name" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="ShortDescription" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Description" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Attribute_1" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Attribute_2" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Attribute_3" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Attribute_4" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Attribute_5" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Attribute_6" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Attributes" nillable="true" type="tns:Attributes" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProductComponent_fromStore">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[List of the available fields for sending components to a product in OpenSi (Store => OpenSi). All fields are required.]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" name="Reference" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="ComponentReference" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Quantity" type="xsd:double" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProductImage_fromStore">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[List of the available fields for sending the default image to a product in OpenSi (Store => OpenSi). All fields are required.]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" name="Reference" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Image" nillable="true" type="xsd:base64Binary" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProductSupplierReference_fromStore">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[List of the available fields for sending a supplier reference of a product in OpenSi (Store => OpenSi). All fields are required.]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" name="ProductReference" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="SupplierReference" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="ProductSupplierReference" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="ProductWholesalePrice" type="xsd:double" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Inventory_fromStore">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[List of the available fields for sending the initial stock of a product in OpenSi (Store => OpenSi). All fields are required.]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" name="Reference" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Quantity" type="xsd:double" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Publication_fromStore">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[List of the available fields for sending the publication/status of a product in OpenSi (Store => OpenSi). All fields are required.]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" name="Reference" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Publication" type="xsd:boolean" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OrderProduct_fromStore">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
					List of the available fields for sending the products of an order in OpenSi (Store => OpenSi).
					A product entry must have at least
						- Reference
						- Name
						- Quantity
						- UnitPrice
						- VATRate
					to be created in OpenSi (required fields).
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" name="Reference" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Name" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Quantity" type="xsd:double" />
			<xsd:element minOccurs="1" maxOccurs="1" name="UnitPrice" type="xsd:double" />
			<xsd:element minOccurs="1" maxOccurs="1" name="VATRate" type="xsd:double" />
			<xsd:element minOccurs="0" maxOccurs="1" name="DiscountAmount" type="xsd:double" />
			<xsd:element minOccurs="0" maxOccurs="1" name="DiscountPercent" type="xsd:double" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Weight" type="xsd:double" />
			<xsd:element minOccurs="0" maxOccurs="1" name="ProductComment" nillable="true" type="xsd:string" />
      <xsd:element minOccurs="0" maxOccurs="1" name="ExpectedDeliveryDate" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Order_fromStore">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
					List of the available fields for sending an order in OpenSi (Store => OpenSi).
					An order entry must have at least
						- OrderNumber
						- Login
						- Identifier
						- OrderDate
						- Shipping (amount)
						- VATRateShipping
						- ShipmentMethod
						- PaymentMethod
						- Billing address
							-- BillingCivility
							-- BillingLastname
							-- BillingFirstname
							-- BillingCompany
							-- BillingAddress_1
							-- BillingZipcode
							-- BillingCity
							-- BillingCountryCode
							-- BillingEmail
						- Delivery address
							-- DeliveryCivility
							-- DeliveryLastname
							-- DeliveryFirstname
							-- DeliveryCompany
							-- DeliveryAddress_1
							-- DeliveryZipcode
							-- DeliveryCity
							-- DeliveryCountryCode
							-- DeliveryEmail
						- at least one product (cf. OrderProduct) to be created in OpenSi (required fields).
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" name="OrderNumber" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="OrderReferenceNumber" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="OrderCartNumber" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="OrderReferenceMarketplace" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Identifier" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Login" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="LoginOpensi" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="ActivityPole" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Title" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="CustomerGroup" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="OrderDate" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="Weight" nillable="true" type="xsd:double" />
			<xsd:element minOccurs="0" maxOccurs="1" name="ExpeditionRule" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="ExpectedDeliveryDate" type="xsd:string" /><!-- Date_Expedition in OpenSi -->
			<xsd:element minOccurs="0" maxOccurs="1" name="EstimatedDeliveryDate" type="xsd:string" /><!-- Date_Livraison (estimated delivery date) in OpenSi -->
      <xsd:element minOccurs="0" maxOccurs="1" name="EstimatedDeliveryDateEnd" type="xsd:string" /><!-- Date_Livraison_Fin (estimated delivery date end) in OpenSi -->
			<xsd:element minOccurs="0" maxOccurs="1" name="ValidOrder" nillable="true" type="xsd:boolean" />
      <xsd:element minOccurs="0" maxOccurs="1" name="BlockedOrder" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="IncludedTaxEdition" type="xsd:boolean" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Shipping" type="xsd:double" />
			<xsd:element minOccurs="1" maxOccurs="1" name="VATRateShipping" type="xsd:double" />
			<xsd:element minOccurs="0" maxOccurs="1" name="DiscountAmount" type="xsd:double" />
			<xsd:element minOccurs="0" maxOccurs="1" name="ShippingDiscountAmount" type="xsd:double" />
			<xsd:element minOccurs="1" maxOccurs="1" name="PaymentMethod" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="ShipmentMethod" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="IntraCommunityVAT" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="BillingCivility" nillable="true" type="xsd:int" />
			<xsd:element minOccurs="1" maxOccurs="1" name="BillingLastname" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="BillingFirstname" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="BillingCompany" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="BillingAddress_1" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="BillingAddress_2" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="BillingAddress_3" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="BillingZipcode" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="BillingCity" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="BillingPhone" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="BillingFax" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="BillingCountryCode" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="BillingEmail" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="DeliveryCivility" nillable="true" type="xsd:int" />
			<xsd:element minOccurs="1" maxOccurs="1" name="DeliveryLastname" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="DeliveryFirstname" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="DeliveryCompany" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="DeliveryAddress_1" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="DeliveryAddress_2" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="DeliveryAddress_3" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="DeliveryZipcode" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="DeliveryCity" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="DeliveryPhone" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="DeliveryFax" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="DeliveryCountryCode" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="DeliveryEmail" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="WithdrawalPoint" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="DeliveryInstructions" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="GiftPackage" nillable="true" type="xsd:boolean" />
			<xsd:element minOccurs="0" maxOccurs="1" name="PrintableComments" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="NonPrintableComments" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="unbounded" name="Product" nillable="true" type="tns:OrderProduct_fromStore" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OrderUpdate_fromStore">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[
					List of the available fields for sending an order in OpenSi (Store => OpenSi).
					An order entry must have at least
						- OrderNumber
						- Login
						- Billing address
							-- BillingCivility
							-- BillingLastname
							-- BillingFirstname
							-- BillingCompany
							-- BillingAddress_1
							-- BillingZipcode
							-- BillingCity
							-- BillingCountryCode
							-- BillingEmail
						- Delivery address
							-- DeliveryCivility
							-- DeliveryLastname
							-- DeliveryFirstname
							-- DeliveryCompany
							-- DeliveryAddress_1
							-- DeliveryZipcode
							-- DeliveryCity
							-- DeliveryCountryCode
							-- DeliveryEmail
						- at least one product (cf. OrderProduct)
					to be updated in OpenSi (required fields).
				]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" name="OrderNumber" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="OrderReferenceNumber" nillable="true" type="xsd:string" />
      <xsd:element minOccurs="0" maxOccurs="1" name="Title" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="BillingCivility" nillable="true" type="xsd:int" />
			<xsd:element minOccurs="1" maxOccurs="1" name="BillingLastname" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="BillingFirstname" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="BillingCompany" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="BillingAddress_1" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="BillingAddress_2" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="BillingAddress_3" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="BillingZipcode" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="BillingCity" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="BillingPhone" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="BillingFax" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="BillingCountryCode" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="BillingEmail" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="DeliveryCivility" nillable="true" type="xsd:int" />
			<xsd:element minOccurs="1" maxOccurs="1" name="DeliveryLastname" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="DeliveryFirstname" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="DeliveryCompany" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="DeliveryAddress_1" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="DeliveryAddress_2" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="DeliveryAddress_3" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="DeliveryZipcode" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="DeliveryCity" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="DeliveryPhone" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="DeliveryFax" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="DeliveryCountryCode" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="DeliveryEmail" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="0" maxOccurs="1" name="WithdrawalPoint" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="unbounded" name="Product" nillable="true" type="tns:OrderProduct_fromStore" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OrdersBalanced_fromStore">
		<xsd:annotation>
