			<xsd:element minOccurs="1" maxOccurs="1" name="TotalAmountExcludingVAT" type="xsd:double" />
			<xsd:element minOccurs="1" maxOccurs="1" name="TotalAmountIncludingVAT" type="xsd:double" />
			<xsd:element minOccurs="1" maxOccurs="1" name="BankRefunded" type="xsd:boolean" />
			<xsd:element minOccurs="1" maxOccurs="unbounded" name="CreditMemos" nillable="true" type="tns:CreditMemoProduct_toStore" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CreditMemoProduct_toStore">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[List of the items on a credit memo sent from OpenSi to the store (OpenSi => Store).]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" name="Reference" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Quantity"  type="xsd:double" />
			<xsd:element minOccurs="1" maxOccurs="1" name="UnitPrice" type="xsd:double" />
			<xsd:element minOccurs="1" maxOccurs="1" name="VATRate" type="xsd:double" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Comment_toStore">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[List of the fields sent from OpenSi to the store to add a new comment (public/private) on an order (OpenSi => Store).]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" name="OrderNumber" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="WebOrderNumber" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="CommentId" type="xsd:int" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Comment" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="CommentDate" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="SendMail" type="xsd:boolean" />
			<xsd:element minOccurs="1" maxOccurs="1" name="VisibleForCustomer" type="xsd:boolean" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OrderReturnState_toStore">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[List of the fields sent from OpenSi to the store to update the status of an order return (OpenSi => Store).]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" name="ReturnNumber" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="WebReturnNumber" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="ReturnState" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OrderReturnDocument_toStore">
		<xsd:annotation>
			<xsd:documentation>
				<![CDATA[List of the fields sent from OpenSi to the store to add a return on an order (OpenSi => Store).]]>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" name="ReturnNumber" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="WebReturnNumber" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Url" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="baseRequest" abstract="true">
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" name="Datetime_Min" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Datetime_Max" nillable="true" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Range_Min" nillable="true" type="xsd:int" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Range_Max" nillable="true" type="xsd:int" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Website_Code" nillable="true" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="baseRequestImage" abstract="true">
		<xsd:complexContent>
			<xsd:extension base="tns:baseRequest">
				<xsd:sequence>
					<xsd:element minOccurs="1" maxOccurs="1" name="All_Image" nillable="true" type="xsd:boolean" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ProductToRelaunch">
		<xsd:sequence>
			<xsd:element name="Reference" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="getProductsRequest">
		<xsd:complexContent>
			<xsd:extension base="tns:baseRequest">
				<xsd:sequence>
					<xsd:element minOccurs="0" maxOccurs="unbounded" name="Product" nillable="true" type="tns:ProductToRelaunch" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="getProductImagesRequest">
		<xsd:complexContent>
			<xsd:extension base="tns:baseRequestImage">
				<xsd:sequence>
					<xsd:element minOccurs="0" maxOccurs="unbounded" name="ProductImage" nillable="true" type="tns:ProductToRelaunch" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="OrderToRelaunch">
		<xsd:sequence>
			<xsd:element name="OrderNumber" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="getOrdersRequest">
		<xsd:complexContent>
			<xsd:extension base="tns:baseRequest">
				<xsd:sequence>
					<xsd:element minOccurs="0" maxOccurs="unbounded" name="Order" nillable="true" type="tns:OrderToRelaunch" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CustomerToRelaunch">
		<xsd:sequence>
			<xsd:element name="CustomerId" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="getCustomersRequest">
		<xsd:complexContent>
			<xsd:extension base="tns:baseRequest">
				<xsd:sequence>
					<xsd:element minOccurs="0" maxOccurs="unbounded" name="Customer" nillable="true" type="tns:CustomerToRelaunch" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="referenceResponse">
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" name="Key" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Code" type="xsd:string" />
			<xsd:element minOccurs="1" maxOccurs="1" name="Message" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
