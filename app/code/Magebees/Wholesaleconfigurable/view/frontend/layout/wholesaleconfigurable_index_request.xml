<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<body>
		<referenceContainer name="content">
			<block class="Magebees\Wholesaleconfigurable\Block\Form" name="wholesaleconfigurableoptions" as="wholesaleconfigurableoptions" template="Magebees_Wholesaleconfigurable::product/list/info.phtml" ifconfig="wholesaleconfigurable/setting/enabled" >
<container name="page.messagess" htmlTag="div" htmlClass="page messages">
                <block class="Magento\Framework\View\Element\Template" name="ajax.message.placeholder" template="Magento_Theme::html/messages.phtml"/>
                <block class="Magento\Framework\View\Element\Messages" name="messages" as="messages" template="Magento_Theme::messages.phtml"/>
            </container></block>
		</referenceContainer>
	

	</body>
</page>




