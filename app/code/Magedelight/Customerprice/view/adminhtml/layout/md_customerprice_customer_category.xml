<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/layout_generic.xsd">
    <update handle="formkey"/>
    <container name="root" label="Root">
        <block class="Magento\Backend\Block\Template" template="Magedelight_Customerprice::customer/category/price/data.phtml" name="data">
            <block class="Magedelight\Customerprice\Block\Adminhtml\Customer\Category\Price\Search" template="customer/category/price/abstract.phtml" name="searchcategory">
                <block class="Magedelight\Customerprice\Block\Adminhtml\Customer\Category\Price\Search\CategoryList" name="catalogprint.categories" template="Magedelight_Customerprice::customer/category/price/categories.phtml" />
            </block>
            <block class="Magedelight\Customerprice\Block\Adminhtml\Customer\CustomerpriceCategory\Items" template="customer/customerpricecategory/items.phtml" name="itemscategory">
                <block class="Magedelight\Customerprice\Block\Adminhtml\Customer\CustomerpriceCategory\Grid\CustomerCategoryPriceGrid" name="items_grid_category"/>
            </block>
        </block>
    </container>
</layout>
