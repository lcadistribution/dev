<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/layout_generic.xsd">
    <container name="root" label="Root">
        <block class="Magento\Backend\Block\Template" template="Magedelight_Customerprice::catalog/price/data.phtml" name="product_customerprice">
            <block class="Magedelight\Customerprice\Block\Adminhtml\Catalog\Price\Search" template="catalog/price/abstract.phtml" name="customer_search">
                <block class="Magedelight\Customerprice\Block\Adminhtml\Catalog\Price\Search\Grid" name="customer_product_search_grid" as="customer_product_search_grid"/>
            </block> 
            <block class="Magedelight\Customerprice\Block\Adminhtml\Catalog\Price\Customerprice\Items" template="catalog/price/customerprice/items.phtml" name="product_items">
                <block class="Magedelight\Customerprice\Block\Adminhtml\Catalog\Price\Customerprice\Grid\CustomerpriceGrid" name="customerprice_grid"/>
            </block>
        </block> 
    </container>
</layout>
