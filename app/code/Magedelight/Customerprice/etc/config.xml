<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <customerprice>
            <general>
            	<enable>0</enable>
            	<hide_price>0</hide_price>
            	<ajax_price>0</ajax_price>
            	<ask_specialprice>0</ask_specialprice>
                <display_ask_specialprice_button_label>Ask Special Price</display_ask_specialprice_button_label>
                <approve_email_template>customerprice_general_approve_email_template</approve_email_template>
                <disapprove_email_template>customerprice_general_disapprove_email_template</disapprove_email_template>
