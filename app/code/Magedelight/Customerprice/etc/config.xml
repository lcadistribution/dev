                <top_enable>0</top_enable>
                <footer_enable>0</footer_enable>
            </general>
            <cron>
            	<import_category>0</import_category>
            	<import_product>0</import_product>
            	<export_category>0</export_category>
            	<export_product>0</export_product>
            </cron>
        </customerprice>
    </default>
</config>
